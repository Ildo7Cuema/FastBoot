{
  "name": "fastboot-factory-reset-web",
  "version": "1.0.0",
  "description": "Aplicação web multiplataforma para restauração de fábrica de dispositivos Android",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "android",
    "fastboot",
    "adb",
    "factory-reset",
    "web",
    "react",
    "express",
    "websocket"
  ],
  "author": "FastBoot Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "socket.io": "^4.7.4",
    "dotenv": "^16.3.1",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
